{
    "manifest_version": 2,
    "name": "Take 5",
    "version": "1.0",
    "description": "Reminding you to take breaks.",

    "icons": {
        "32": "icon32.png",
        "128": "icon128.png"
    },

    "browser_action": {
        "default_icon": {
            "16": "icon16.png",
            "24": "icon24.png",
            "32": "icon32.png"
        },

        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "storage"
    ],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }


}
